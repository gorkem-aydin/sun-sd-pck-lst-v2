{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.sun.sd.packinglist",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZSD_PACKING_LIST_SRV": {
				"uri": "/sap/opu/odata/sap/ZSD_PACKING_LIST_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			},
			"annotation": {
				"type": "ODataAnnotation",
				"uri": "annotation.xml",
				"settings": {
					"localUri": "annotation.xml"
				}
			}
		}
	},
	"sap.ui": {
		"fullWidth": true,
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.sun.sd.packinglist.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.ui.table": {},
				"sap.ui.fl": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sun.sd.packinglist.i18n.i18n"
				}
			},
			"": {
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"defaultUpdateMethod": "PUT"
				},
				"dataSource": "ZSD_PACKING_LIST_SRV",
				"preload": true
			},
			"viewModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "./model/viewModel.json",
				"preload": true
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sun.sd.packinglist.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [
				{
					"name": "OrderList",
					"pattern": "",
					"target": [
						"OrderList"
					]
				},
				{
					"pattern": "OrderDetail/:orderId:/:orderStatus:",
					"name": "orderDetail",
					"target": [
						"orderDetail"
					]
				},
				{
					"name": "goodsMovement",
					"pattern": "GoodsMovement/:vbeln:",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"goodsMovement"
					]
				}
			],
			"targets": {
				"OrderList": {
					"viewType": "XML",
					"viewName": "OrderList",
					"viewLevel": 1
				},
				"orderDetail": {
					"viewName": "OrderDetail",
					"viewId": "orderDetail",
					"viewLevel": 2,
					"title": "{i18n>worklistViewTitle}"
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2,
					"title": "{i18n>objectViewTitle}"
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"goodsMovement": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "GoodsMovement",
					"title": "{i18n>goodsMovementTitle}",
					"viewId": "goodsMovement",
					"viewLevel": 1
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsd_packing/webapp",
		"_version": "1.1.0"
	}
}